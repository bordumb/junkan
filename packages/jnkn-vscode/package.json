{
  "name": "jnkn-vscode",
  "displayName": "Jnkn LSP Client",
  "description": "Graph based blast radius highlighting.",
  "version": "0.0.1",
  "publisher": "jnkn",
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jnkn-vscode.restart",
        "title": "Jnkn: Restart LSP Server"
      },
      {
        "command": "jnkn-vscode.showOutput",
        "title": "Jnkn: Show Output Channel"
      }
    ],
    "configuration": {
      "title": "Jnkn",
      "properties": {
        "jnkn.pythonPath": {
          "type": "string",
          "default": "python3",
          "description": "Path to Python interpreter"
        },
        "jnkn.serverPath": {
          "type": "string",
          "default": "",
          "description": "Path to jnkn LSP server.py (leave empty to use installed package)"
        },
        "jnkn.trace.server": {
          "type": "string",
          "enum": ["off", "messages", "verbose"],
          "default": "verbose",
          "description": "Trace communication with the language server"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.75.0",
    "typescript": "^5.1.3",
    "eslint": "^8.41.0"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}